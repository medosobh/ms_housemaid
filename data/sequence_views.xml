<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for sales tickets -->
        <record id="housemaid_sales_tickets_seq"
            model="ir.sequence">
            <field name="name">Housmaid Sales Tickets</field>
            <field name="code">housemaid.sales.tickets</field>
            <field name="prefix">ST/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id"
                eval="False"/>
        </record>
        <!-- Sequences for operation tickets -->
        <record id="housemaid_operation_tickets_seq"
            model="ir.sequence">
            <field name="name">Housmaid Operation Tickets</field>
            <field name="code">housemaid.operation.tickets</field>
            <field name="prefix">OT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id"
                eval="False"/>
        </record>
    </data>
</odoo>