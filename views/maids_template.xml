<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="maids_page"
        name="Maids">
        <t t-call="website.layout">
            <div class="container">
                hi, external office your maid's cv list is here 
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <td>
                                Name:
                            </td>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="maids"
                            t-as="maid">
                            <tr>
                                <td>
                                    <t t-esc="maid.name"/>
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </table>
            </div>
        </t>
    </template>

</odoo>
